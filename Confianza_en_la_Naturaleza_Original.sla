                <tab FONT="DejaVu Sans Bold" FONTSIZE="11"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Al rechazar la existencia, se pierde su verdadera naturaleza,"/>
                <breakline/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="al aferrarse al vaco, se niega su autntico significado."/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
