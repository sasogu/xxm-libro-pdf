                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Cuando la mente no construye,"/>
                <breakline/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="los diez mil fenmenos estn libres de error. "/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
