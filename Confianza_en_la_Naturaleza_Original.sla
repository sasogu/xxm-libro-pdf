                <tab FONT="DejaVu Sans Bold" FONTSIZE="11"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="J xio tong d Wng ju jng ji"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH=" "/>
                <tab FONT="DejaVu Sans Bold" FONTSIZE="11"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Lo infinitamente pequeo es idntico a lo infinitamente grande,"/>
                <breakline/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="cuando se olvidan los lmites y se disuelven las fronteras."/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
