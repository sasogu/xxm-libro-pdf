                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="La visin limitada, la duda y la desconfianza;"/>
                <breakline/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Generan unas veces indecisin y otras apresuramiento."/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
