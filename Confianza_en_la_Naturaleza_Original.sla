                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Qin kng zhun bin ji yu wng jin"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH=" "/>
                <tab FONT="DejaVu Sans Bold" FONTSIZE="11"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="Los cambios que parecen tener lugar en el vaco,"/>
                <breakline/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11" CH="surgen de una percepcin equivocada creada por la ignorancia."/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="-5.64094488188976" VOR="2.86299212598425" NACH="2.86299212598425" BCOLOR="None"/>
